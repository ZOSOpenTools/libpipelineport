export ZOPEN_GIT_URL="https://github.com/Distrotech/libpipeline.git"
export ZOPEN_GIT_DEPS="git make curl"
export ZOPEN_TYPE="GIT"

zopen_check_results()
{
  dir="$1"
  pfx="$2"
  chk="$1/$2_check.log"

  # Echo the following information to guage build health
  echo "actualFailures:0"
  echo "totalTests:1"
  echo "expectedFailures:0"
}

zopen_append_to_env()
{
  # echo envars outside of PATH, MANPATH, LIBPATH
}
